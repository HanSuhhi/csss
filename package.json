{
  "name": "@hansuhhi-don/csss",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && rm dist/csss.js",
    "preview": "vite preview",
    "develop": "pnpm build && pnpm docs:dev",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh ./scripts/deploy.sh",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "stylelint": "stylelint src/**/*.css",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vuepress/client": "2.0.0-beta.51",
    "@vuepress/plugin-back-to-top": "2.0.0-beta.51",
    "@vuepress/plugin-register-components": "2.0.0-beta.51",
    "@vuepress/utils": "2.0.0-beta.51",
    "@yanyu-fe/vuepress-plugin-code-block": "^0.2.0",
    "happy-dom": "^7.5.12",
    "husky": "^8.0.1",
    "lodash-es": "^4.17.21",
    "rollup-plugin-delete": "^2.0.0",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.24.3",
    "vue-tsc": "1",
    "vuepress": "2.0.0-beta.51"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./dist/style.css": "./dist/style.css"
  }
}